    "babel-preset-es2015": "6.22.0",
    "babel-preset-es2017": "6.22.0",
    "babel-preset-stage-2": "6.22.0",
    "codecov": "1.0.1",
    "flow-bin": "0.39.0",
    "flow-typed": "2.0.0",
    "flow-types": "1.0.0",
    "jest": "18.1.0",
    "jsdoc-to-markdown": "3.0.0",

{
  "name": "http-call",
  "description": "make http requests",
  "version": "1.0.8",
  "author": "Jeff Dickey @dickeyxxx",
  "bugs": "https://github.com/dickeyxxx/http-call/issues",
  "dependencies": {
    "babel-polyfill": "6.23.0"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-2": "6.22.0",
    "codecov": "1.0.1",
    "flow-bin": "0.39.0",
    "flow-copy-source": "1.1.0",
    "flow-types": "1.0.0",
    "jest": "18.1.0",
    "jsdoc-babel": "0.3.0",
    "jsdoc-to-markdown": "3.0.0",
    "nock": "9.0.6",
    "np": "2.12.0",
    "rimraf": "2.6.0",
    "standard": "8.6.0"
  },
  "homepage": "https://github.com/dickeyxxx/http-call",
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "keywords": [
    "http",
    "request",
    "rest"
  ],
  "license": "ISC",
  "main": "lib/http.js",
  "repository": "dickeyxxx/http-call",
  "scripts": {
    "build": "babel src -d lib --ignore '*.test.js' --source-maps inline",
    "clean": "rimraf lib",
    "copy-flow": "flow-copy-source -v -i '*.test.js' src lib",
    "doc": "jsdoc2md -c doc/jsdoc2md.json > README.md",
    "prepare": "npm run clean && npm run build && npm run copy-flow",
    "test": "jest && flow && standard",
    "version": "npm run doc && git add README.md"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "flow-typed"
    ]
  }
}
